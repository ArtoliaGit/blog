<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.artolia.blog.mapper.role.RoleMapper">

    <insert id="save" parameterType="com.artolia.blog.domain.auth.Role" keyProperty="roleId">
        insert into role(role_name, role_description, create_time)
        values(#{roleName}, #{roleDescription}, now())
    </insert>
</mapper>